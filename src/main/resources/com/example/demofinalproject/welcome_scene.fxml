<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Label?>
<VBox alignment="CENTER" prefWidth="300" prefHeight="300" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.demofinalproject.HelloController">
    <Label text="Welcome, select action:" />
    <Button text="Doctor" fx:id="doctorButton" onAction="#handleDoctorButtonAction"/>
    <Button text="Patient" fx:id="patientButton" onAction="#handlePatientButtonAction"/>
    <Button text="Register" fx:id="registerButton" onAction="#handleRegisterButtonAction"/>
</VBox>